  //MUSIC PLAYER
  let currentPlaylist = null;
  class Track {
    constructor(title, artist, coverLink, audioLink) {
      this.title = title;
      this.artist = artist;
      this.coverLink = coverLink;
      this.audioLink = audioLink;
    }
    getTitle() {
      return this.title;
    }
  
    getArtist() {
      return this.artist;
    }
  
    getCoverLink() {
      return this.coverLink;
    }
  
    getAudioLink() {
      return this.audioLink;
    }
  }
  
  class Playlist {
    constructor() {
      this.tracks = [];
      this.currentTrackIndex = 0;
    }
  
    addTrack(track) {
      this.tracks.push(track);
    }
  
    getCurrentTrack() {
      return this.tracks[this.currentTrackIndex];
    }
  
    playNext() {
      this.currentTrackIndex = (this.currentTrackIndex + 1) % this.tracks.length;
    }
  
    playPrevious() {
      this.currentTrackIndex = (this.currentTrackIndex - 1 + this.tracks.length) % this.tracks.length;
    }
  }
  window.Playlist=Playlist
  window.Track=Track
  
	function changePlaylist(newPlaylist) {
	  if (!newPlaylist || !(newPlaylist instanceof Playlist)) {
		console.error("Invalid playlist provided.");
		return;
	  }
	  
	  // Update the current playlist and reset the track index
	  currentPlaylist = newPlaylist;
	  currentPlaylist.currentTrackIndex = 0;

	  // Update the UI and start playback
	  const audio = document.getElementById("audio");
	  updateUI(currentPlaylist);
	  audio.src = currentPlaylist.getCurrentTrack().audioLink;
	  audio.play();
	}
  window.changePlaylist=changePlaylist
  
  function updateUI(playlist) {
      const currentTrack = playlist.getCurrentTrack();
      var player = document.getElementById("player");
      player.querySelector("#cover").src = currentTrack.getCoverLink();
      player.querySelector("#title").textContent = currentTrack.getTitle();
      player.querySelector("#artist").textContent = currentTrack.getArtist();
  }
  
  window.updateUI=updateUI
  
  
