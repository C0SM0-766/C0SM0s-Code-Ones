    function updateProgressBar() {
      const audio = document.getElementById("audio");
      const progress = (audio.currentTime / audio.duration) * 100;
      try{progressBar.value = progress}
    catch{}
    }
  
   
	let isDragging = false; // Track whether the user is dragging the progress bar

	progressBar.addEventListener("mousedown", function (event) {
	  isDragging = true;
	  updateAudioCurrentTime(event); // Update the audio time immediately on click
	});

	document.addEventListener("mousemove", function (event) {
	  if (isDragging) {
		updateAudioCurrentTime(event);
	  }
	});

	document.addEventListener("mouseup", function () {
	  if (isDragging) {
		isDragging = false;
	  }
	});

	function updateAudioCurrentTime(event) {
	  const audio = document.getElementById("audio");
	  const progressBarRect = progressBar.getBoundingClientRect();
	  const clickPositionX = event.clientX - progressBarRect.left;
	  const progressWidth = progressBarRect.width;
	  const newTime = (clickPositionX / progressWidth) * audio.duration;

	  // Set the new time to the audio element and update the progress bar
	  audio.currentTime = Math.max(0, Math.min(newTime, audio.duration)); // Ensure time is within bounds
	  progressBar.value = (audio.currentTime / audio.duration) * 100;
	}
  
    const audio = document.createElement("audio");
    audio.id = "audio";
    audio.src = playlist.getCurrentTrack().audioLink;
  
	audio.addEventListener("ended", function () {
	  if (currentPlaylist) {
		currentPlaylist.playNext();
		updateUI(currentPlaylist);
		audio.src = currentPlaylist.getCurrentTrack().audioLink;
		audio.play();
	  } else {
		console.error("No playlist!");
	  }
	});
	
	audio.addEventListener("timeupdate", () => {
        if (audio.duration) {
            const progress = (audio.currentTime / audio.duration) * 100;
            progressBar.value = progress;
        }
    });
  
    document.body.appendChild(audio);
    audio.play();
  
    // Update the UI when the page loads
    updateUI(playlist);
  }
  
